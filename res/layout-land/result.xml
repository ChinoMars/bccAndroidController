<?xml version="1.0" encoding="utf-8"?><LinearLayout         xmlns:android="http://schemas.android.com/apk/res/android"        android:orientation="vertical"        android:layout_width="fill_parent"        android:layout_height="fill_parent"        android:padding="10dip"        android:id="@+id/land_result_page">    <TextView android:layout_width="match_parent"              android:layout_height="wrap_content"              android:text="@string/txt_title_bbc"              android:id="@+id/result_title"              android:gravity="center"/>    <!--<Spinner-->        <!--android:layout_width="match_parent"-->        <!--android:layout_height="wrap_content"-->        <!--android:id="@+id/spn_bg"-->        <!--android:entries="@array/arr_spn_bg">-->    <!--</Spinner>-->    <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:orientation="horizontal"            android:layout_marginLeft="10dip"            android:layout_marginRight="10dip"            android:layout_weight="1">        <LinearLayout                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:orientation="vertical">            <TableLayout                    android:layout_height="fill_parent"                    android:layout_width="wrap_content"                    android:layout_weight="1">                <!--<TableRow>-->                    <!--<TextView-->                    <!--android:id="@+id/result_label"-->                    <!--android:layout_width="wrap_content"-->                    <!--android:layout_height="wrap_content"-->                    <!--android:text="@string/txt_result"/>-->                <!--</TableRow>-->                <TableRow                        android:layout_marginBottom="10dip">                    <TextView                        android:id="@+id/cnt_label"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="@string/txt_cnt"/>                    <EditText                            android:id="@+id/edt_cnt"                            android:layout_height="30dp"                            android:layout_width="wrap_content"                            android:inputType="numberDecimal"                            android:hint="0.0"                            android:focusable="false"                            android:paddingBottom="1dip"/>                </TableRow>                <TableRow                        android:layout_marginBottom="10dip">                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="@string/txt_loss"/>                    <EditText                            android:id="@+id/edt_loss"                            android:layout_width="wrap_content"                            android:layout_height="30dp"                            android:inputType="numberDecimal"                            android:focusable="false"                            android:hint="0.0"                            android:paddingBottom="1dip"/>                    <TextView                            android:layout_height="wrap_content"                            android:layout_width="wrap_content"                            android:text="@string/txt_dB"                            android:layout_weight="1"/>                </TableRow>                <!--不显示FSR-->                <!--<TableRow>-->                    <!--<TextView-->                        <!--android:layout_width="wrap_content"-->                        <!--android:layout_height="wrap_content"-->                        <!--android:text="@string/fst_text"/>-->                    <!--<EditText-->                            <!--android:id="@+id/edt_fst"-->                            <!--android:layout_width="wrap_content"-->                            <!--android:layout_height="wrap_content"-->                            <!--android:numeric="decimal"-->                            <!--android:editable="false"-->                            <!--android:text="0.0"/>-->                    <!--<TextView-->                            <!--android:layout_width="wrap_content"-->                            <!--android:layout_height="wrap_content"-->                            <!--android:text="@string/mhz_text"/>-->                <!--</TableRow>-->                <TableRow                    android:layout_marginBottom="10dip">                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="@string/txt_dl"/>                    <EditText                            android:id="@+id/edt_dl"                            android:layout_width="wrap_content"                            android:layout_height="30dp"                            android:inputType="numberDecimal"                            android:focusable="false"                            android:hint="0.0"                            android:layout_weight="2"                            android:paddingBottom="1dip"/>                    <TextView                        android:layout_height="wrap_content"                        android:layout_width="wrap_content"                        android:text="@string/txt_metre"/>                </TableRow>                <TableRow                    android:layout_marginBottom="10dip">                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="@string/txt_n"/>                    <EditText                            android:id="@+id/edt_n"                            android:layout_height="30dp"                            android:layout_width="wrap_content"                            android:inputType="numberDecimal"                            android:hint="1.44000"                            android:focusable="true"                            android:focusableInTouchMode="true"                            android:paddingBottom="1dip"/>                </TableRow>                <SeekBar                        android:layout_height="wrap_content"                        android:layout_width="wrap_content"                        android:indeterminate="false"                        android:id="@+id/skb_n"                        android:max="9"                        android:progress="0"                        android:layout_gravity="bottom"                        android:layout_weight="1"/>            </TableLayout>        </LinearLayout>        <!--自定义画图区域-->        <!--<Button-->                <!--android:layout_width="fill_parent"-->                <!--android:layout_height="match_parent"-->                <!--android:text="1"-->                <!--android:id="@+id/test"-->                <!--android:layout_marginLeft="20dip"/>-->        <!--<TextView-->        <ScrollView            android:layout_width="fill_parent"            android:layout_height="match_parent"            android:text="1"            android:id="@+id/curve_drawer"            android:layout_marginLeft="20dip"            android:layout_gravity="right"            android:fillViewport="false"> <!--改成自定义绘图时去掉这行-->            <TextView                android:layout_width="fill_parent"                android:layout_height="fill_parent"                android:id="@+id/tvLog"                android:gravity="left"                android:text="Connecting..."/>        </ScrollView>    </LinearLayout>    <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:orientation="vertical"            android:gravity="center"            android:layout_weight="2">        <LinearLayout                android:layout_width="wrap_content"                android:layout_height="0dp"                android:orientation="horizontal"                android:layout_weight="1"                android:gravity="center">            <RadioGroup                android:layout_width="fill_parent"                android:layout_height="wrap_content"                android:id="@+id/rdiog_rangeSetter"                android:orientation="horizontal">                <RadioButton                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="@string/txt_rdio_long"                        android:layout_marginLeft="20dip"                        android:id="@+id/rdio_long"/>                <RadioButton                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="@string/txt_rdio_mid"                        android:layout_marginLeft="20dip"                        android:id="@+id/rdio_mid"/>                <RadioButton                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="@string/txt_rdio_short"                        android:layout_marginLeft="20dip"                        android:id="@+id/rdio_short"/>            </RadioGroup>        </LinearLayout>        <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="horizontal">            <Button                android:layout_height="wrap_content"                android:layout_width="wrap_content"                android:text="@string/txt_btn_measure"                android:id="@+id/btn_measure"                android:layout_weight="1"/>            <Button android:layout_width="wrap_content" android:layout_height="wrap_content"                android:layout_weight="1"                android:text="@string/txt_btn_paramset"                android:id="@+id/btn_paramsetter"/>            <Button                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="@string/txt_btn_save"                android:id="@+id/btn_savedata"                android:layout_weight="1"/>        </LinearLayout>    </LinearLayout></LinearLayout>